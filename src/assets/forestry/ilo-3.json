    "dialogue-06": {
      "type": "dialogue",
      "character": "Vic",
      "text": "Let us examine selected species under the family Fabaceae. We will start with AYANGILE, Acacia confusa.",
      "next": "trigger-02"
    },
    "dialogue-07": {
      "type": "dialogue",
      "character": "Vic",
      "text": "Ayangile's distinct features are:\n\nIts brownish black color, moderately numerous pores that are moderately small, presence of white deposits and vasincentric parenchyma.",
      "next": "trigger-03",
      "prev": "trigger-04"
    },

    "dialogue-01": {
      "type": "dialogue",
      "character": "Narrator",
      "text": "Supervisor Vic is orienting their newly hired Forester, Sue.",
      "next": "trigger-02"
    },
    "dialogue-02": {
      "type": "dialogue",
      "character": "Vic",
      "text": "Hi Sue! Welcome to your first day at work.",
      "next": "trigger-03",
      "prev": "trigger-04"
    },
    "dialogue-03": {
      "type": "dialogue",
      "character": "Vic",
      "text": "I will assign you at the checkpoint today where you need to verify the identity of the logs harvested by different logging companies and collect the corresponding forest charges.",
      "next": "trigger-05",
      "prev": "trigger-06"
    },
    "dialogue-04": {
      "type": "dialogue",
      "character": "Vic",
      "text": "We need to look at its gross features, specifically its physical and structural features.",
      "next": "trigger-07",
      "prev": "trigger-08"
    },
    "dialogue-05": {
      "type": "dialogue",
      "character": "Vic",
      "text": "But before deployment, let us go to our xylarium to review these features.",
      "next": "trigger-09",
      "prev": "trigger-010"
    },

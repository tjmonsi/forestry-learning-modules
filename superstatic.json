{
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/forms",
      "destination": "/index.html"
    },
    {
      "source": "/narrative-editing",
      "destination": "/index.html"
    },
    {
      "source": "/event-editing",
      "destination": "/index.html"
    }
  ]
}